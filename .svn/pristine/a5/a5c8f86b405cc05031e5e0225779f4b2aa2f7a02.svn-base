<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
     	<typeAlias type="com.nichijou.springmvcdemo.model.User" alias="user"/>
     	<typeAlias type="com.nichijou.springmvcdemo.model.Likevote" alias="likevote"/>
     	<typeAlias type="com.nichijou.springmvcdemo.model.Tag" alias="tag"/>
     	<typeAlias type="com.nichijou.springmvcdemo.model.Post" alias="post"/>
     	<typeAlias type="com.nichijou.springmvcdemo.model.PostTag" alias="posttag"/>
     </typeAliases>
     
     <mappers>
         <!-- 注册mapper文件 -->
         <mapper resource="mapper/userMapper.xml"/>
         <mapper resource="mapper/likevoteMapper.xml"/>
         <mapper resource="mapper/tagMapper.xml"/>
         <mapper resource="mapper/postMapper.xml"/>
     </mappers>
     
     
</configuration>
